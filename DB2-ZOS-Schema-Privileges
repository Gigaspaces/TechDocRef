--GSHI user Db2 z/OS database privileges
PERMIT DB2A.SYSIBM.SYSCOLAUTH.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSCOLUMNS.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSDATABASE.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSDATATYPES.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSDBAUTH.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSDUMMY1.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSFOREIGNKEYS.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSINDEXES.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSKEYS.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSLEVELUPDATES.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSROUTINES.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSPACKSTMT.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSPARMS.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSRELS.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSSCHEMAAUTH.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSSYNONYMS.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSTABAUTH.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSTABCONST.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSTABLES.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSIBM.SYSTABLESPACE.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ)
PERMIT DB2A.MONITOR2 CLASS(MDSNSM) ID(GSHI) ACCESS(READ)
PERMIT DB2A.GSHI.CHCRLRSP.EXECUTE CLASS(MDSNSP) ID(GSHI) ACCESS(READ)
PERMIT DB2A.BINDADD CLASS(MDSNSM) ID(GSHI) ACCESS(READ)
PERMIT DB2A.NULLID.CREATEIN CLASS(MDSNCL) ID(GSHI) ACCESS(READ)
PERMIT DB2A.GSHI.CREATEIN CLASS(MDSNSC) ID(GSHI) ACCESS(READ)
PERMIT DB2A.GSHI.ALTERIN CLASS(MDSNSC) ID(GSHI) ACCESS(READ)
PERMIT DB2A.GSHI.DROPIN CLASS(MDSNSC) ID(GSHI) ACCESS(READ)
PERMIT DB2A.GSHI.CREATEIN CLASS(MDSNCL) ID(GSHI) ACCESS(READ)
PERMIT DB2A.SYSPROC.WLM_REFRESH.EXECUTE CLASS(MDSNSP) ID(GSHI) ACCESS(READ)
/*
Create a schema by creating a database, tablespace and table.
The CREATE statements are SQL statements for Db2.
Â */
CREATE DATABASE GSHIDB;
CREATE TABLESPACE GSHITS CCSID EBCDIC IN GSHIDB;
CREATE TABLE GSHI.DUMMY (C1 INT) IN GSHIDB.GSHITS;
PERMIT DB2A.GSHI.DUMMY.SELECT CLASS(MDSNTB) ID(GSHI) ACCESS(READ);
PERMIT DB2A.GSHI.DUMMY.ALTER CLASS(MDSNTB) ID(GSHI) ACCESS(READ);

